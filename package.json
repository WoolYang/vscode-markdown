{
    "name": "wool-markdown",
    "displayName": "wool-markdown",
    "description": "markdown preview tool",
    "version": "0.0.1",
    "publisher": "wool",
    "engines": {
        "vscode": "^1.24.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onLanguage:markdown",
        "onCommand:extension.openPrewiew"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.openPrewiew",
                "title": "prewiew"
            }
        ]
    },
    "main": "./out/extension",
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
